

<%- include('./partials/head.ejs')  -%> 
<body>
    <button id="myBtn">Create <i class='far fa-id-card' style='font-size:24px'></i></button>

  
    <div class="container">
     <%- include('./partials/modal.ejs')  -%> 
    <table class="table table-striped">
        <tr>
            <% for( let index = 0; index < list.length; index++ ) { %>
                <th><%= list[index] %> </th>
            
            <% } %>
            <th>action</th>
          

        </tr>
        <% for( element of companies ) { %>
        <tr class="body">
         
            <% for(key in element) { %>
              
 
                <td> <a class="hover-7" href="http://localhost:5005/company/<%= element._id %>">   <%= element[key] %>    </a> </td>
               
            
            <% } %>
            <td class="btn updateBtn"><div class="button convex-concave">Update</div></td>
            <td class="btn DeleteBtn" ><div class="button convex-concave">Delete</div></td> 
        </tr>
        <% } %>
        
        
      </table>

      <form action="http://localhost:5005/company/companiesPage" method="POST">
      <label for="start">Created at
        <input name="start" id="start" type="date">
      </label>
     
      <label for="end">end point
        <input name="end" id="end" type="date">
      </label>
      <input type="submit" value="submit" id="submit">
    </form>
     
    </div>
    

    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
    <script src="../javascripts/company.js"></script>
   <script src="../javascripts/modal.js"></script>
</body>
</html>